{
  "name": "fourseasons.kg",
  "version": "0.1.0",
  "author": "FYS Studios",
  "description": "FOUR SEASONS - TOURS AND TRAVEL",
  "private": true,
  "main": "server.js",
  "scripts": {
    "serve:dev": "npm run dev",
    "dev": "nodemon --watch app --watch bootstrap --watch config --watch .env -x node server.js",
    "build": "node ./ace nuxtbuild",
    "start": "cross-env NODE_ENV=production node server.js",
    "precommit": "npm run lint",
    "lint": "standard && eslint --ext .js,.vue resources/"
  },
  "dependencies": {
    "@adonisjs/ace": "^5.0.0",
    "@adonisjs/auth": "^3.0.1",
    "@adonisjs/bodyparser": "^2.0.2",
    "@adonisjs/cors": "^1.0.5",
    "@adonisjs/fold": "^4.0.7",
    "@adonisjs/framework": "^5.0.4",
    "@adonisjs/ignitor": "^2.0.5",
    "@adonisjs/lucid": "^5.0.2",
    "@adonisjs/mail": "^3.0.5",
    "@adonisjs/redis": "^2.0.4",
    "@adonisjs/session": "^1.0.20",
    "@adonisjs/shield": "^1.0.6",
    "@adonisjs/validator": "^5.0.3",
    "@nuxtjs/axios": "^5.0.0",
    "@nuxtjs/proxy": "^1.1.4",
    "axios": "^0.18.0",
    "cross-env": "^3.1.4",
    "highcharts": "^6.0.7",
    "izitoast": "^1.2.0",
    "jquery": "^3.3.1",
    "moment": "^2.20.1",
    "mysql": "^2.15.0",
    "nuxt": "latest",
    "vue-carousel": "^0.6.5",
    "vue-highcharts": "^0.0.10",
    "vue-i18n": "^7.3.2",
    "vue-notifications": "^0.9.0",
    "vue-scroll-to": "^2.2.0",
    "vue-select": "^2.4.0",
    "vue-social-sharing": "^2.3.3",
    "vue-types": "^1.1.3",
    "youch": "^2.0.4"
  },
  "standard": {
    "global": [
      "use",
      "make"
    ]
  },
  "autoload": {
    "App": "./app",
    "TOURS": "App/Models/Tours",
    "OTHERS": "App/Models/Other",
    "AUTHS": "App/Models/Auths",
    "IMAGES": "App/Models/Images",
    "NEWS": "App/Models/News"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "eslint": "^3.13.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-html": "^2.0.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "eslint-plugin-vue": "^4.0.0",
    "less": "^3.0.1",
    "less-loader": "^4.0.6",
    "nodemon": "^1.11.0",
    "standard": "^8.6.0",
    "svg-inline-loader": "^0.8.0",
    "vue-svg-loader": "^0.5.0"
  }
